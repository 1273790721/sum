$(function(){ 
    $.ajax({
        url:'myrule',//"{:url('index/myrule')}",
        type:'post',
        data:{'uid':1},
        dataType:'json',
        success:function(json){
       //     layer.msg(json.msg);
                var navs = json.data;
                alert(navs[0]['title']);
        }
    });
});
